#include<stdio.h>
int main()
{
  int n=0,key,i;
  printf("Enter the size of array:");
  scanf("%d",&n);
  int a[n];
  printf("Enter array elements:");
   for(i=0;i<n;i++)
   {
     scanf("%d",&a[i]);
     }
    printf("Enter element you search:");
    scanf("%d",&key);
    int found=0;
    for(i=0;i<n;i++)
    {
      if(a[i]==key)
      {
        found=1;
        break;
        }
    }
    if(found){
       printf("Element %d found at position %d\n",key,i);
    }
    else{
    printf("Element %d is not found",key);
    }
    return 0;
    }



*****************************************************************************************************************************************************************************************************************************


#OUTPUT

Enter the size of array:5
Enter array elements:1
2
3
4
5
Enter element yo search:4
Element 4 found are position 3


=== Code Execution Successful ===
